//  RESOURCE IDENTIFIER
NAME    AWIZ // 4 letter ID

//  INCLUDES
#include <eikon.rh>
#include "shadows.hrh"
#include "shadows.rls"
#include <avkon.rsg>
#include <avkon.rh>
#include <avkon.mbg>
#include <appinfo.rh>

//  RESOURCE DEFINITIONS
RESOURCE RSS_SIGNATURE { }
RESOURCE TBUF { buf="Shadows"; }
RESOURCE EIK_APP_INFO
	{
    menubar = r_shadows_menubar;
    cba = R_AVKON_SOFTKEYS_OPTIONS_BACK;
	}

//----------------------------------------------------
//
//    r_shadows_menubar
//
//----------------------------------------------------
//
RESOURCE MENU_BAR r_shadows_menubar
{
    titles =
        {
            MENU_TITLE
            {
                menu_pane = r_shadows_exit;
            }
        };
}

//----------------------------------------------------
//
//    r_shadows_exit
//
//----------------------------------------------------
//
RESOURCE MENU_PANE r_shadows_exit
{
    items =
        {
            MENU_ITEM
            {
                command = EAknCmdExit;
                txt = STRING_r_shadows_exit;
            }
        };
}

//----------------------------------------------------
//
//    r_shadows_localisable_app_info
//
//----------------------------------------------------
//
RESOURCE LOCALISABLE_APP_INFO r_shadows_localisable_app_info
    {
    short_caption = STRING_r_app_short_caption_string;
    caption_and_icon =
    CAPTION_AND_ICON_INFO
        {
        caption         = STRING_r_app_caption_string;
        number_of_icons = 1;
        icon_file       = "\\resource\\apps\\shadows_icon.mif";
        };
    }

// End of File