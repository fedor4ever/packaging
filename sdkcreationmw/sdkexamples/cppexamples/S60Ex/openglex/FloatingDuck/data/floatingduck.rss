// RESOURCE IDENTIFIER
NAME AWIZ // 4 letter ID

//  INCLUDES
#include <eikon.rh>
#include "floatingduck.hrh"
#include "floatingduck.rls"
#include <avkon.rsg>
#include <avkon.rh>
#include <avkon.mbg>
#include <appinfo.rh>

//  RESOURCE DEFINITIONS
RESOURCE RSS_SIGNATURE { }
RESOURCE TBUF { buf="FloatingDuck"; }
RESOURCE EIK_APP_INFO
    {
    hotkeys=r_floatingduck_hotkeys;
    menubar=r_floatingduck_menubar;
    cba=R_AVKON_SOFTKEYS_OPTIONS_BACK;
    }

//----------------------------------------------------
//
//    r_floatingduck_hotkeys
//
//----------------------------------------------------
//
RESOURCE HOTKEYS r_floatingduck_hotkeys
    {
    control=
        {
        HOTKEY { command=EAknCmdExit; key='e'; }
        };
    }

//----------------------------------------------------
//
//    r_floatingduck_menubar
//
//----------------------------------------------------
//
RESOURCE MENU_BAR r_floatingduck_menubar
    {
    titles=
        {
        MENU_TITLE { menu_pane=r_floatingduck_menu; txt="File"; }
        };
    }

//----------------------------------------------------
//
//    r_floatingduck_menu
//
//----------------------------------------------------
//
RESOURCE MENU_PANE r_floatingduck_menu
    {
    items=
        {
        MENU_ITEM { command=EAknCmdExit; txt="Exit"; }
        };
    }

//----------------------------------------------------
//
//    r_floatingduck_app_menu
//
//----------------------------------------------------
//
RESOURCE MENU_PANE r_floatingduck_app_menu
    {
    items=
        {
        MENU_ITEM { command=EAknCmdExit; txt="Exit"; }
        };
    }

//----------------------------------------------------
//
//    r_floatingduck_localisable_app_info
//
//----------------------------------------------------
//
RESOURCE LOCALISABLE_APP_INFO r_floatingduck_localisable_app_info
    {
    short_caption = STRING_r_app_short_caption_string;
    caption_and_icon =
    CAPTION_AND_ICON_INFO
        {
        caption         = STRING_r_app_caption_string;
        number_of_icons = 1;
        icon_file       = "\\resource\\apps\\floatingduck_icon.mif";
        };
    }

