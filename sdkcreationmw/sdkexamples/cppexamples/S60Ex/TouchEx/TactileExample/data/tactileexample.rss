NAME TACT    // 4 letter ID


//  INCLUDES
#include <eikon.rh>
#include <avkon.rh>
#include <avkon.rsg>
#include <appinfo.rh>
#include "tactileexample.rls"

//  RESOURCE DEFINITIONS
// ---------------------------------------------------------------------------
//
//    Define the resource file signature
//    This resource should be empty.
//
// ---------------------------------------------------------------------------
//
RESOURCE RSS_SIGNATURE
    {
    }

//----------------------------------------------------------------------------
//
//    Default Document Name
//
//----------------------------------------------------------------------------

RESOURCE TBUF r_default_document_name
    {
    buf = STRING_r_tactile_caption_string;
    }


// ---------------------------------------------------------------------------
//
//    Define default menu and CBA key.
//
// ---------------------------------------------------------------------------
//
RESOURCE EIK_APP_INFO
    {
    cba = R_AVKON_SOFTKEYS_EXIT;
    }

// ---------------------------------------------------------------------------
//
// Resources for button.
//
// ---------------------------------------------------------------------------
//
RESOURCE TBUF32 r_tact_button_text { buf=STRING_r_tact_button_text; }

// ---------------------------------------------------------------------------
//
// r_tactileexample_localisable_app_info
//
// ---------------------------------------------------------------------------
//
RESOURCE LOCALISABLE_APP_INFO r_tactileexample_localisable_app_info
    {
    short_caption = STRING_r_tactile_short_caption_string;
    caption_and_icon =
    CAPTION_AND_ICON_INFO
        {
        caption = STRING_r_tactile_caption_string;

        number_of_icons = 1;
        icon_file = "\\resource\\apps\\tactileexample_aif.mif";
        };
    }

